<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="365dp"
        android:layout_height="565dp"
        app:srcCompat="@drawable/pig_map_3"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="0dp" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="53dp"
        android:layout_height="50dp"
        app:srcCompat="@drawable/pig_child"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="64dp" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="78dp"
        android:layout_height="60dp"
        app:srcCompat="@drawable/pig_mother"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="266dp"
        android:layout_marginTop="435dp" />

    <ImageButton
        android:id="@+id/imageButton16"
        android:layout_width="60dp"
        android:layout_height="63dp"
        android:backgroundTint="#0000"
        android:onClick="showAdditionalMenu"
        android:scaleType="fitXY"
        android:tint="#212121"
        app:srcCompat="@drawable/points_additional_menu"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="284dp"
        android:layout_marginTop="0dp" />

    <View
        android:id="@+id/myRectangleView2"
        android:layout_width="408dp"
        android:layout_height="162dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="-511dp"
        android:background="@drawable/rectangle"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="parent"
        tools:layout_editor_absoluteX="-1dp"
        android:elevation="2dp"/>

    <ImageButton
        android:id="@+id/imageButton18"
        android:layout_width="82dp"
        android:layout_height="77dp"
        android:backgroundTint="#0000"
        android:onClick="hideAdditionalMenu"
        android:tint="#ffffff"
        app:srcCompat="@drawable/arrow_back_turned"
        app:layout_constraintRight_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginRight="-365dp"
        android:layout_marginTop="37dp"
        android:elevation="2dp"/>

    <View
        android:id="@+id/myRectangleView3"
        android:layout_width="4dp"
        android:layout_height="160dp"
        android:layout_marginLeft="259dp"
        android:layout_marginTop="-495dp"
        android:background="@drawable/border"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="parent"
        android:elevation="2dp"/>

    <View
        android:id="@+id/view15"
        android:layout_width="4dp"
        android:layout_height="160dp"
        android:layout_marginLeft="123dp"
        android:layout_marginTop="-511dp"
        android:background="@drawable/border"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="parent"
        android:elevation="2dp"/>

    <ImageButton
        android:id="@+id/imageButton19"
        android:layout_width="82dp"
        android:layout_height="82dp"
        android:layout_marginLeft="152dp"
        android:layout_marginTop="0dp"
        android:backgroundTint="#0000"
        android:onClick="goToStartPage"
        android:tint="#ffffff"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@id/imageButton18"
        app:srcCompat="@drawable/home"
        android:elevation="2dp"/>

    <ImageButton
        android:id="@+id/imageButton20"
        android:layout_width="82dp"
        android:layout_height="82dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="0dp"
        android:backgroundTint="#0000"
        android:onClick="turnOffMusic"
        android:tint="#ffffff"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@id/imageButton18"
        app:srcCompat="@drawable/ic_volume_up_turned"
        android:elevation="2dp"/>

    <ImageButton
        android:id="@+id/imageButton21"
        android:layout_width="82dp"
        android:layout_height="82dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="0dp"
        android:backgroundTint="#0000"
        android:onClick="turnOnMusic"
        android:tint="#ffffff"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@id/imageButton18"
        app:srcCompat="@drawable/ic_volume_off_turned"
        android:elevation="2dp"/>

    <ImageView
        android:id="@+id/imageView28"
        android:layout_width="226dp"
        android:layout_height="258dp"
        android:layout_marginLeft="78dp"

        android:layout_marginTop="184dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/smile" />

    <ImageView
        android:id="@+id/imageView29"
        android:layout_width="226dp"
        android:layout_height="302dp"
        android:layout_marginLeft="78dp"

        android:layout_marginTop="160dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/sad" />

</android.support.constraint.ConstraintLayout>